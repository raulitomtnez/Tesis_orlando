<?xml version="1.0" encoding="utf-8"?><!--Created by OrlanDroyd on 31/10/2019.-->
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:clickable="false"
    android:focusable="false"
    app:cardCornerRadius="2dp"
    app:cardElevation="4dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false"
        android:focusable="false"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="5dp">

        <TextView
            android:id="@+id/tv_numero_estanque"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="numero Estanque" />

        <TextView
            android:id="@+id/tv_areaEstanque"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="areaEstanque" />

        <TextView
            android:id="@+id/tv_ciCamaronicultor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="camronnicultor" />
        <TextView
            android:id="@+id/tv_nombreCamaronicultor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="camronnicultor" />

        <TextView
            android:id="@+id/tv_apellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="camronnicultor" />

        <TextView
            android:id="@+id/tv_idGranja"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="nombre Granja" />


    </LinearLayout>

</android.support.v7.widget.CardView>
