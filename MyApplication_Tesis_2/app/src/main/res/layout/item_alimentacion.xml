<?xml version="1.0" encoding="utf-8"?><!--Created by OrlanDroyd on 31/10/2019.-->
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:clickable="false"
    android:focusable="false"
    app:cardCornerRadius="2dp"
    app:cardElevation="4dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false"
        android:focusable="false"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="5dp">

        <TextView
            android:id="@+id/tv_fechaAlimentacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="fechaAlimentacion" />

        <TextView
            android:id="@+id/tv_sesionManana1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="sesionManana1" />

        <TextView
            android:id="@+id/tv_sesionManana2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="sesionManana2" />

        <TextView
            android:id="@+id/tv_sesiontarde1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="sesiontarde1" />

        <TextView
            android:id="@+id/tv_sesiontarde2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="sesiontarde2" />

        <TextView
            android:id="@+id/tv_observaciones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="observaciones" />

        <TextView
            android:id="@+id/tv_idFechaSiembra"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="FechaSiembra" />

        <TextView
            android:id="@+id/tv_idNumero_estanque"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="numero_estanque" />

        <TextView
            android:id="@+id/tv_idTipoPienso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:focusable="false"
            android:gravity="center"
            android:textSize="18sp"
            tools:text="nmbreTipoPienso" />


    </LinearLayout>

</android.support.v7.widget.CardView>
